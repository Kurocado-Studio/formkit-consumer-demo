{
  "name": "@kurocado-studio/formkit-consumer-form-demo",
  "version": "0.0.0-semantically-released",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "commitlint": "pnpm exec commitlint --edit .git/COMMIT_EDITMSG",
    "clean": "rm -rf node_modules pnpm.lock dist && pnpm i",
    "dev": "cross-env VITE_NODE_ENV=local vite",
    "eslint-check": "eslint --max-warnings=0 . --debug",
    "eslint-fix": "eslint --max-warnings=0 . --fix --no-warn-ignored",
    "lint": "eslint --max-warnings=0 . --no-warn-ignored",
    "prepare": "husky install",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --check . --write",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc --noEmit --pretty",
    "setup": "pnpm clean && pnpm i && pnpm build"
  },
  "lint-staged": {
    "*": ["pnpm run prettier-fix"],
    "**/*.{ts,tsx}": [
      "pnpm run prettier-fix",
      "pnpm run eslint-fix",
      "pnpm run lint"
    ]
  },
  "dependencies": {
    "@kurocado-studio/formkit-ui-models": "1.0.0-develop.1",
    "@kurocado-studio/formkit-ui-react-renderer": "1.0.0-develop.8",
    "@kurocado-studio/systemhaus-motion-react": "1.0.0-develop.9",
    "@kurocado-studio/systemhaus-react": "1.1.0-develop.13",
    "axios": "^1.13.2",
    "lodash-es": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@kurocado-studio/styleguide": "2.6.0-develop.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.13",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.21",
    "commitlint": "^20.1.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.23.0",
    "husky": "^9.1.7",
    "import": "^0.0.6",
    "lint-staged": "^16.2.6",
    "loader-utils": "3.2.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "semantic-release": "^25.0.2",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.2",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "react": "19.0.0",
      "react-dom": "19.0.0"
    }
  },
  "resolutions": {
    "esbuild": "0.25.12",
    "rollup": "4.52.0"
  },
  "eslintConfig": {
    "extends": ["plugin:storybook/recommended"]
  }
}
